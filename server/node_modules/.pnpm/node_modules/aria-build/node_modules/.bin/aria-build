#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules/aria-build/bin/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules/aria-build/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules/aria-build/bin/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules/aria-build/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/aria-build@0.4.5_aria-fs@0.4.5_typescript@5.9.3/node_modules:/mnt/c/Users/USER/Documents/dev/micro-commerce-app/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/aria-build.js" "$@"
else
  exec node  "$basedir/../../bin/aria-build.js" "$@"
fi
